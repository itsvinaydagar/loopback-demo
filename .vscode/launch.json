{
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "outputCapture": "std",
      "name": "Auth Service Debug",
      "program": "${workspaceFolder}/dist/index.js",
      "cwd": "${workspaceFolder}",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "runtimeArgs": ["--preserve-symlinks", "--preserve-symlinks-main"],
      "protocol": "inspector",
      "sourceMaps": true,
      "autoAttachChildProcesses": true
    },
  ]
}
