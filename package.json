{
  "name": "loopback-demo",
  "version": "0.0.1",
  "description": "The Learning App",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "posttest": "yarn run lint",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "set NODE_ENV=development && node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "dev": "set NODE_ENV=development && nodemon -r source-map-support/register . --watch src"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "“itsvinaydagar” <“vinay.dagar@sourcefuse.com”>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.0.1",
    "@loopback/context": "^4.0.1",
    "@loopback/core": "^3.0.1",
    "@loopback/logging": "^0.7.1",
    "@loopback/repository": "^4.0.1",
    "@loopback/rest": "^11.0.1",
    "@loopback/rest-explorer": "^4.0.1",
    "@loopback/service-proxy": "^4.0.1",
    "dotenv": "^11.0.0",
    "loopback-connector-postgresql": "^5.0.1",
    "nodemon": "^2.0.15",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.1",
    "@loopback/eslint-config": "^12.0.1",
    "@loopback/testlab": "^4.0.1",
    "@types/node": "^10.17.60",
    "eslint": "^8.6.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.5.4"
  }
}
